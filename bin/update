#!/usr/bin/env bash


[ "$1" = "--all" ] && echo_separate "makepkg vscode insiders" \
                && cd ~/.cache/yay/visual-studio-code-insiders-bin \
                && makepkg -si --noconfirm

echo_separate "yay -Syu"
yay -Syyu

echo_separate "tldr -u"
tldr -u

echo_separate "zinit"
zinit update

